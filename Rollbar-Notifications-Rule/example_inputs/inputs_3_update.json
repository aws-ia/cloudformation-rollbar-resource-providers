{
  "ProjectAccessToken": "ROLLBAR_PROJECT_TOKEN_INPUT",
  "PagerDuty": {
    "Trigger": "reactivated_item",
    "Filters": [
      {
        "Type": "environment",
        "Operation": "eq",
        "Value": "production"
      },
      {
        "Type": "level",
        "Operation": "gte",
        "Value": "error"
      },
      {
        "Type": "environment",
        "Operation": "neq",
        "Value": "dev"
      },
      {
        "Type": "environment",
        "Operation": "neq",
        "Value": "staging"
      }
    ]
  }
}